{
  "name": "jelou-backend-challenge",
  "version": "0.0.1",
  "description": "Jelou Backend challenge",
  "author": "Cristian Pisco <cristian.pisco@outlook.com>",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeder:generate": "sequelize-cli seed:generate --seeders-path db/seeders --name",
    "seeder:run": "sequelize-cli db:seed:all",
    "seeder:delete": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "nodejs",
    "bootstrap",
    "express"
  ],
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "4.19.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  }
}
